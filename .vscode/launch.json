{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Benchmark Parallel",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/performance_benchmark.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--preset", "H100X2",
                "--num-requests", "50",
                "--concurrency", "10",
                "--mode", "parallel",
                "--batch-interval", "1.0"
            ],
        },
        {
            "name": "Benchmark Sequential",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/performance_benchmark.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--preset", "H100X2",
                "--num-requests", "50",
                "--concurrency", "10",
                "--mode", "sequential"
            ],
        }

    ]
}